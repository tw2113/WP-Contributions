{
	"name": "block",
	"version": "0.1.0",
	"description": "Example static block scaffolded with Create Block tool.",
	"author": "The WordPress Contributors",
	"license": "GPL-2.0-or-later",
	"main": "/assets/block/build/index.js",
	"scripts": {
		"build": "wp-scripts build --webpack-src-dir=assets/block/src --output-path=assets/block/build",
		"format": "wp-scripts format --webpack-src-dir=assets/block/src --output-path=assets/block/build",
		"lint:css": "wp-scripts lint-style --webpack-src-dir=assets/block/src --output-path=assets/block/build",
		"lint:js": "wp-scripts lint-js --webpack-src-dir=assets/block/src --output-path=assets/block/build",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start --webpack-src-dir=assets/block/src --output-path=assets/block/build"
	},
	"devDependencies": {
		"@wordpress/scripts": "^28"
	},
	"dependencies": {
		"@wordpress/element": "^6",
		"@wordpress/i18n": "^5",
		"@wordpress/icons": "^10",
		"npm-check-updates": "^16.14.20"
	}
}
